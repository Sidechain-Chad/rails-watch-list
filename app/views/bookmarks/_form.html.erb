<%= form_with(model: [ @bookmark.list, @bookmark ]) do |form| %>
  <div>
    <%= form.label :comment %><br>
    <%= form.text_field :comment %>
  </div>
  <div>
    <%= form.label :movie_id, 'Movie' %><br>
    <%= form.collection_select :movie_id, Movie.all, :id, :title %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
